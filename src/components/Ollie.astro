---
import { Image } from "astro:assets";
import ollie from "@src/images/ollie.jpg";
const classList = [
  "mx-auto",
  "rounded-full",
  "border-4",
  "border-stone-300",
  "dark:border-stone-600",
  Astro.props.class, // optional
];
---

<Image
  src={ollie}
  width={600}
  height={600}
  alt=""
  loading="eager"
  class:list={classList}
/>
